<%= form_for @rating_type do |f| %>
  <p>
    <%= f.label :value %>
    <%= f.text_field :value, value: @rating_type.value, class: "value" %>
  </p>

  <p>
    <%= f.label :priority %>
    <%= f.number_field :priority, value: @rating_type.priority %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
